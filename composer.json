{
    "name": "languara/fuelphp",
    "keywords": ["Languara", "FuelPHP", "Translation", "Localization", "Localize", "Internationalization", "Internationalize", "Translate", "l10n", "i18n"],
    "homepage": "https://languara.com/localization/fuelphp",
    "description": "Languara localization plugin for FuelPHP 1.x. Easily upload, translate, and synchronize your language content using Languara's Cloud Based Translation Management Platform.",
    "type" : "fuel-package",
    "license": "MIT",
    "type" : "library",
    "authors": [
        {
            "name": "Languara Development Team",
            "email": "support@languara.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "languara/core": "~1.0"
    },
    "autoload":    {
        "psr-4": {
            "Languara\\": "",
            "Fuel\\Tasks\\": "tasks/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "installer-name": "languara",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
